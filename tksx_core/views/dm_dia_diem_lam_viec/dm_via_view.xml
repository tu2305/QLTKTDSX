<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_via_tree" model="ir.ui.view">
            <field name="name">via.tree</field>
            <field name="model">dm.via</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="V敲겨">
                    <field name="ma_via"/>
                    <field name="ten_via"/>
                </tree>
            </field>
        </record>
        <record id="view_via_form" model="ir.ui.view">
            <field name="name">via.form</field>
            <field name="model">dm.via</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="V敲겨">
                    <sheet>
                        <group col="4">
                            <field name="ma_via"/>
                            <field name="ten_via"/>
                        </group>
                        <group>
                            <field name="mo_ta"/>
                        </group>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>
        <record id="view_via_search" model="ir.ui.view">
            <field name="name">via.search</field>
            <field name="model">dm.via</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <search string="V敲겨">
                    <field name="ten_via"/>
                    <field name="ma_via"/>
                </search>
            </field>
        </record>
        <record model="ir.actions.act_window" id="act_open_via_view">
            <field name="name">V敲겨</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">dm.via</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_via_search"/>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_via_view_form">
            <field name="act_window_id" ref="act_open_via_view"/>
            <field name="sequence" eval="20"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_via_form"/>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_via_view_tree">
            <field name="act_window_id" ref="act_open_via_view"/>
            <field name="sequence" eval="10"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_via_tree"/>
        </record>
    </data>
</odoo>
